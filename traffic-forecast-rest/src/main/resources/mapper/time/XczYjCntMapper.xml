<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.embracesource.traffic.time.dao.XczYjCntDao">
    <select id="getAmountByStation" resultType="java.lang.Long">
        select yj_dd
        from xcz_yj_cnt
        where xcz = #{station}
