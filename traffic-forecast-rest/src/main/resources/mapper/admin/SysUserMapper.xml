<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.embracesource.traffic.admin.dao.SysUserDao">

    <select id="getUserByName" resultType="com.embracesource.traffic.admin.domain.SysUserDO">
        select *
        from sys_user
        where user_name = #{userName}
    </select>
</mapper>